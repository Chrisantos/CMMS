                                <h4 class="card-title m-b-0">{{spare.spare_name}}</h4>
                            </div>
                            <table class="table">
                                <thead>
                                </thead>
                                <tbody>
                                    <tr>
                                        <td><strong>Equipment</strong></td>
                                        <td class="text-success">{{spare.equipt}}</td>
                                    </tr>
                                    <tr>
                                        <td><strong>Equipment Location</strong></td>
                                        <td class="text-primary">{{spare.equipt_loc}}</td>
                                    </tr>
                                    <tr>
                                        <td><strong>Spare ID</strong></td>
                                        <td class="text-secondary">{{spare.sn}}</td>
                                    </tr>
                                    <tr>
                                        <td><strong>Quantity</strong></td>
                                        <td class="text-info">{{spare.quantity}}</td>
                                    </tr>
                                    <tr>
                                        <td><strong>Description</strong></td>
                                        <td class="text-primary">{{spare.description}}</td>
                                    </tr>
                                    <tr>
                                        <td><strong>Cost</strong></td>
