                            <div class="card card-hover">
                                <div class="box bg-cyan text-center">
                                    <h1 class="font-light text-white"><i class="mdi mdi-collage"></i></h1>
                                    <h6 class="text-white">Log books</h6>
                                </div>
                            </div>
                        </a>
                        
                    </div>
                    <!-- Column -->
                    <div class="col-md-6 col-lg-3">
                        <a href="/work-order">
                            <div class="card card-hover">
                                <div class="box bg-white text-center">
                                    <h1 class="font-light text-blue"><i class="mdi mdi-border-outside"></i></h1>
                                    <h6 class="text-blue">Work Order</h6>
                                </div>
                            </div>
                        </a>
                        
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-6">
                         <div class="card">
                            <div class="card-body">
                                <h4 class="card-title">Engineers</h4>
                            </div>
                            <div class="comment-widgets scrollable">
                                {{#if engineers}}
                                {{#each engineers}}
                                    <!-- Comment Row -->
                                    <div class="d-flex flex-row comment-row m-t-0">
                                        <div class="p-2"><img src="/assets/images/users/1.jpg" alt="user" width="50" class="rounded-circle"></div>
                                        <div class="comment-text w-100">
                                            <h6 class="font-medium">{{name}}</h6>
                                            <span class="m-b-15 d-block">{{username}}</span>
                                            <div class="comment-footer">
                                                <span class="text-muted float-right">{{phone}}</span> 
                                            <a class="btn-rectangle btn-sm btn-secondary  text-white" href="/hr/engineer/{{username}}">View</a>
                                            <a class="btn-rectangle btn-sm btn-danger  text-white" href="/hr/remove-engineer-admin/{{username}}">Delete</a>
                                            </div>
                                        </div>
                                    </div>
                                {{/each}}
                                {{else}}
                                        <h6 class="text-center"><strong>No Engineers</strong></h6>
                                {{/if}}
                            </div>
                        </div>
                         <!-- Card -->
                        
                    </div>
                    <div class="col-md-6">
                         <!-- card -->
                        
                        <!-- card new -->
                        <div class="card">
                            <div class="card-body">
                                <h4 class="card-title">Schedules</h4>
                                <div class="todo-widget scrollable">
                                    <ul class="list-task todo-list list-group m-b-0" data-role="tasklist">
                                        {{#if schedules}}
                                        {{#each schedules}}
                                            <li class="list-group-item todo-item" data-role="task">
                                                <div class="custom-control custom-checkbox">
                                                    <input type="checkbox" class="custom-control-input" id="{{date}}" onchange="window.location.href='/maintenance/delete-corrective-admin/{{schedule_id}}'">
                                                    <label class="custom-control-label todo-label" for="{{date}}">
                                                        <span class="todo-desc">{{equipt}}</span><span class="badge badge-pill badge-primary float-right">{{frequency}} </span>
                                                    </label>
                                                </div>
                                                <div class="item-date">{{date}}</div>
                                            </li>
                                        {{/each}}
                                        {{else}}
                                            <h6 class="text-center"><strong>No Schedules yet</strong></h6>
                                        {{/if}}
                                    </ul>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
